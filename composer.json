{
    "name": "league/flysystem-adapter-decorator",
    "description": "A base implementation of a flysystem adapter decorator",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank de Jonge",
            "email": "info@frenky.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "^1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\AdapterDecorator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Flysystem\\AdapterDecorator\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
